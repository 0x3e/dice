.dices
  margin 6px 3px 6px 2px
.dice
  padding 12px
  margin-right -1px
  float left
  border 1px solid
  border-collapse collapse
  text-align center
.roll,
.delete, 
.label 
  padding 12px
.rolling
  visibility: hidden
.out > div 
  padding 12px
  margin 12px
  border 2px solid
  display inline-block
.max
  background rgb(255, 0, 0)
  background rgba(255, 0, 0, 0.6)
.min
  background rgb(0, 0, 255)
  background rgba(0, 0, 255, 0.3)
